<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_MechChimp</defName>
		<label>chimp mecharmor</label>
		<description>Mech ape garment crafted using Spacer machines.</description>
		<graphicData>
			<texPath>Apparel/Chimp/MechChimp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>3000</costStuffCount>
		<techLevel>Spacer</techLevel>
		<costList>
			<AdvancedComponent>10</AdvancedComponent>
			<Component>50</Component>
			
		</costList>
		
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>1000</MaxHitPoints>
			<WorkToMake>420000</WorkToMake>
			<Mass>15</Mass>
			<ArmorRating_Blunt>0.75</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.85</ArmorRating_Sharp>
			<ArmorRating_Heat>0.70</ArmorRating_Heat>
			<ArmorRating_Electric>0.75</ArmorRating_Electric>
			<Insulation_Cold>-30</Insulation_Cold>
			<Insulation_Heat>50</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.20</MoveSpeed>
			<WorkSpeedGlobal>-0.20</WorkSpeedGlobal>
		</equippedStatOffsets>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/Chimp/MechChimp</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Ape</li>
			</tags>
		</apparel>
		<recipeMaker>
			<!--<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>-->
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ApeMechArmor</researchPrerequisite>
			<recipeUsers>
				<li>TableMachining</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>RGBA(0.4,0.3,0.15,1)</only>
				</li>
				<li>
					<weight>15</weight>
					<only>RGBA(0.6,0.45,0.18,1)</only>
				</li>
				<li>
					<weight>20</weight>
					<only>RGBA(0.8,0.6,0.23,1)</only>
				</li>
			</options>
		</colorGenerator>
</ThingDef>

